{
  "name": "real_estate",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android:prod": "cross-env EXPO_PUBLIC_ENV=production expo run:android",
    "ios:prod": "cross-env EXPO_PUBLIC_ENV=production expo run:ios",
    "build:android": "cross-env EXPO_PUBLIC_ENV=production eas build -p android",
    "build:ios": "cross-env EXPO_PUBLIC_ENV=production eas build -p ios",
    "prebuild:android": "npx expo prebuild -p android --clean",
    "prebuild:ios": "npx expo prebuild -p ios --clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.10.0",
    "expo": "~53.0.12",
    "expo-application": "^6.1.4",
    "expo-blur": "^14.1.5",
    "expo-constants": "~17.1.6",
    "expo-device": "~7.1.4",
    "expo-document-picker": "^13.1.6",
    "expo-font": "^13.3.1",
    "expo-haptics": "^14.1.4",
    "expo-image": "^2.3.0",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "^7.1.5",
    "expo-network": "^7.1.5",
    "expo-notifications": "^0.31.3",
    "expo-router": "^5.1.0",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "^14.2.0",
    "nativewind": "^4.1.23",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-export-namespace-from": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@react-native-community/cli": "^18.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "react-native-dotenv": "^3.4.11"
  },
  "private": true,
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|expo-router)"
    ]
  }
}
